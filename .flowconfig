[ignore]
.*/node_modules/.*
.*/build/.*
.*/src/serviceWorker.js*
all=warn
[include]
[libs]
[lints]
sketchy-null-bool=off
sketchy-null-number=off
sketchy-null-string=off
untyped-type-import=error
[options]
esproposal.optional_chaining=enable
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.system.node.resolve_dirname=src/
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^constants\(.*\)$' -> '<PROJECT_ROOT>/src/constants/\1'
module.name_mapper='^containers\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^pages\(.*\)$' -> '<PROJECT_ROOT>/src/pages/\1'
module.name_mapper='^store\(.*\)$' -> '<PROJECT_ROOT>/src/store/\1'
module.name_mapper='^types\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'
module.name_mapper='^utils\(.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
module.name_mapper='^assets\(.*\)$' -> '<PROJECT_ROOT>/src/assets/\1'
module.name_mapper='^selectors\(.*\)$' -> '<PROJECT_ROOT>/src/selectors/\1'
module.name_mapper='^src\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
[strict]
